<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- 第一层  上部和左部偏移一定距离-->
    <item
        android:left="2.66dp"
        android:top="2.66dp">
        <shape android:shape="rectangle">
            <solid android:color="#77c3c3c3" />
            <!-- 描边 -->
            <stroke
                android:width="8dp"
                android:color="#77c3c3c3" />
            <corners android:radius="20dp"/>
            <size android:width="@dimen/abc_progress_bar_height_material"
                android:height="@dimen/abc_progress_bar_height_material"/>
        </shape>
    </item>
    <!-- 第二层 下部和有部偏移一定距离-->
    <item
        android:bottom="0.66dp"
        android:right="0.66dp">
        <shape>
            <solid android:color="#ff8a49" />
            <!-- 描边 -->
            <stroke android:width="0.33dp" android:color="#dedede" />
            <size android:width="@dimen/abc_progress_bar_height_material"
                android:height="@dimen/abc_progress_bar_height_material"/>
            <corners android:radius="20dp"/>
        </shape>
    </item>
</layer-list>